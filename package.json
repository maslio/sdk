{
  "name": "@maslio/kit",
  "type": "module",
  "version": "0.1.0",
  "description": "My personal kit for a nuxt project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maslio/kit.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspector": "eslint-config-inspector",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "docker": "docker compose -f ./docker/docker-compose.yaml up -d"
  },
  "dependencies": {
    "@ai-sdk/google": "^0.0.15",
    "@ai-sdk/openai": "^0.0.14",
    "@antfu/eslint-config": "^2.17.0",
    "@directus/sdk": "^16.0.0",
    "@directus/types": "^11.1.1",
    "@floating-ui/vue": "^1.0.6",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/device": "^3.1.1",
    "@types/lodash-es": "^4.17.12",
    "@unocss/eslint-plugin": "^0.60.0",
    "@unocss/nuxt": "^0.60.0",
    "@vueuse/nuxt": "^10.9.0",
    "ai": "^3.1.14",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "delay": "^6.0.0",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "fluent-vue": "^3.6.0",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "maska": "^2.1.11",
    "nanoid": "^5.0.7",
    "nuxt-icon": "^0.6.10",
    "nuxt-particles": "^0.3.0",
    "ofetch": "^1.3.4",
    "ohash": "^1.1.3",
    "ollama-ai-provider": "^0.7.0",
    "openai": "^4.47.1",
    "pathe": "^1.1.2",
    "pretty-bytes": "^6.1.1",
    "scule": "^1.3.0",
    "ufo": "^1.5.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "unplugin-fluent-vue": "^1.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.4.8",
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.11",
    "changelogen": "^0.5.5",
    "concurrently": "^8.2.2",
    "nuxt": "npm:nuxt-nightly@latest",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
